{
  "opts": {
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "includePattern": ".+\\.(m|c)?js(doc|x)?$"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "Github repo": {
        "href": "https://github.com/stonemjs/middleware",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    }
  }
}